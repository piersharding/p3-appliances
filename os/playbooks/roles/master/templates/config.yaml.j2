---
# private agents - all except first two
agent_list:
{% for item in groups["shared_services_compute"][2:] %}
- {{ hostvars[item].ansible_host }}
{% endfor %}

# Use this bootstrap_url value unless you have moved the DC/OS installer assets.
bootstrap_url: http://{{ hostvars["shared_services-controller-0"].ansible_host }}:5001
cluster_name: shared_services
exhibitor_storage_backend: static
master_discovery: static
#ip_detect_public_filename: /home/ubuntu/genconf/ip-detect

# one master - first node
master_list:
- {{ hostvars["shared_services-compute-0"].ansible_host }}

# one public agent 2nd node
public_agent_list:
- {{ hostvars["shared_services-compute-1"].ansible_host }}
#{% for item in groups["shared_services_compute"] %}
#- {{ hostvars[item].ansible_host }}
#{% endfor %}

# nameserver 10.60.253.10
# search alaskalocal
resolvers:
{% for item in dcos_name_servers %}
- {{ item }}
{% endfor %}

ssh_port: 22
ssh_user: ubuntu
use_proxy: 'false'
#http_proxy: http://<proxy_host>:<http_proxy_port>
#https_proxy: https://<proxy_host>:<https_proxy_port>
#no_proxy: 
#- 'foo.bar.com'
#- '.baz.com'
