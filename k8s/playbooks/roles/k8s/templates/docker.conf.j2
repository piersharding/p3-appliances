[Service]
MountFlags=shared
ExecStart=
# -H tcp://0.0.0.0:2375
ExecStart=/usr/bin/dockerd -H unix:///var/run/docker.sock \
          --label nodetype={{ host_grouping }} --experimental=true \
          --metrics-addr=0.0.0.0:9323 --insecure-registry=localhost:5000 \
          --insecure-registry={{ docker_repo }}
